{
    "manifest_version": 3,
    "name": "Spendee Blacklist",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["*://*.spendee.mattle.online/*"],
        "js": ["scripts/content.js"]
      }
    ],
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    }
  }
  